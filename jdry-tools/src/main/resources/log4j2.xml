<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug">


    <appenders>

        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level - %msg%n"/>
        </Console>


    </appenders>


    <loggers>


        <Root level="all">
            <AppenderRef ref="console" level="all"/>
        </Root>
    </loggers>
</Configuration>