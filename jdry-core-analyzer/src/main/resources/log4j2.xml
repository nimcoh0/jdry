<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug">
    <appenders>
        <Console name="console" >
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </appenders>
    <loggers>
        <Root level="trace">
            <AppenderRef ref="console" />
        </Root>
    </loggers>
</Configuration>